{
  "name": "TherapyCorner",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "ios:beta": "bundle exec fastlane ios beta",
    "ios:beta:staging": "sh config.sh staging && yarn ios:beta",
    "ios:beta:production": "sh config.sh production && yarn ios:beta",
    "android:index:bundle": "rm -f android/app/src/main/assets/index.android.bundle && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw",
    "android:bundleRelease": "cd android && ./gradlew clean && ./gradlew bundleRelease",
    "android:beta": "bundle exec fastlane android beta",
    "android:beta:staging": "sh config.sh staging && yarn android:index:bundle && yarn android:beta",
    "android:beta:production": "sh config.sh production && yarn android:index:bundle && yarn android:beta",
    "android:live:production": "sh config.sh production && yarn android:index:bundle && yarn android:bundleRelease"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@react-native-async-storage/async-storage": "^1.13.4",
    "@react-native-community/datetimepicker": "^3.0.9",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^11.0.0",
    "@react-native-firebase/messaging": "^11.0.0",
    "@react-native-picker/picker": "^1.9.11",
    "@react-navigation/compat": "^5.3.13",
    "@react-navigation/drawer": "^5.12.3",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.2",
    "dayjs": "^1.10.4",
    "formik": "^2.2.6",
    "galio-framework": "^0.7.1",
    "graphql": "^15.5.0",
    "haversine": "^1.1.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-config": "^1.4.2",
    "react-native-deep-linking": "^2.2.0",
    "react-native-device-info": "^8.1.7",
    "react-native-elements": "^3.2.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.6.1",
    "react-native-modal-datetime-picker": "^9.2.0",
    "react-native-pager-view": "^5.1.2",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.17.1",
    "react-native-tab-view": "^3.0.0",
    "react-native-vector-icons": "^8.0.0",
    "react-native-version-check": "^3.4.2",
    "react-native-version-number": "^0.3.6",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
